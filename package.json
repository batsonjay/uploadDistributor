{
  "name": "upload-distributor",
  "version": "0.1.0",
  "description": "A system for uploading and distributing audio content to multiple platforms",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md,js,jsx,json}\"",
    "check-types": "turbo run check-types",
    "test": "turbo run test"
  },
  "devDependencies": {
    "@types/node": "^22.15.3",
    "@types/uuid": "^10.0.0",
    "prettier": "^3.5.3",
    "turbo": "^2.5.2",
    "typescript": "5.8.2",
    "uuid": "^11.1.0"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@10.9.2",
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
